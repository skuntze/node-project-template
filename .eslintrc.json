{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": false,
        "node": true
    },
    "globals": {},
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-dupe-keys": "warn",
        "consistent-return": "warn",
        "eol-last": "error",
        "default-case": "warn",
        "eqeqeq": "warn",
        "func-call-spacing": "off",
        "no-alert": "error",
        "semi": "error"
    }
}
